extends layout

block content
  h2 Suggestions
  ul
    each item in content
      li
        div(style='width : 800px; height : 150px;')
          div(style='float : left; width : 30%')
            img.image(src=item.Images[0].url_170x135)
          div(style='float : left; width : 70%')
            a.item(href=item.url)= item.title